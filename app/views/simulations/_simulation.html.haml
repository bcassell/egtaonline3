%tr
  %td.fixed= link_to simulation.state, simulation_path(simulation)
  %td= link_to simulation.assignment, profile_path(simulation.profile_id)
  %td.fixed= link_to simulation.simulator_fullname, simulator_path(simulation.simulator_id) #stimulator_path(Profile.find_by(simulation.profile_id).simulator_instance_id)
  %td.fixed= link_to simulation.id, simulation_path(simulation)
  %td.fixed= link_to (simulation.job_id != nil ? simulation.job_id : "N/A"), simulation_path(simulation)
